    "@vue/cli-service": "3.12.1",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/test-utils": "1.0.0-beta.30",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
